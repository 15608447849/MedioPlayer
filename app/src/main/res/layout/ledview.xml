<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:mview="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height ="match_parent"
       >
    <!-- android:layout_below="@+id/ledview_clock_week" -->
    <com.wos.play.rootdir.model_application.ui.ComponentLibrary.image.MeImageView
        android:id="@+id/show_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:src="@mipmap/ic_launcher"
        />
    <com.wos.play.rootdir.model_application.ui.ComponentLibrary.weather.AutoTextView
        android:id="@+id/switcher02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        mview:textSize="24sp"
        android:layout_below="@id/show_icon"
        />
    </RelativeLayout>


